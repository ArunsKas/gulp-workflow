{
  "name": "Gulpfile Setup",
  "description": "",
  "authors": [
    "mohamdio"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mohamdio/gulpfile-setup",
  "moduleType": [],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.1.4",
    "foundation-sites": "~6.0.5",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v4.0.0-alpha.zip"
  },
  "overrides": {
    "bootstrap": {
      "main": [
      "scss/**/*.scss",
      "dist/css/bootstrap.css",
      "dist/js/bootstrap.js",
      "dist/js/bootstrap.min.js"
      ]
    },
    "foundation-sites": {
      "main": [
      "scss/**/*.scss",
      "dist/foundation.css",
      "dist/*.js"
      ]
    },
    "jquery": {
      "main": [
      "dist/jquery.js",
      "dist/jquery.min.js"
      ]
    }
  }
}